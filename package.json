    "eslint-plugin-prettier": "3.2.0",
    "prettier": "^2.3.2",
    "prisma": "^3.14.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9"
  }
